issn = {15206106},
journal = {Journal of Physical Chemistry B},
number = {27},
pages = {13670--13677},
pmid = {16821896},
title = {{Hydrogen bonding and the cryoprotective properties of glycerol/water mixtures}},
volume = {110},
year = {2006}
}
