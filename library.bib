isbn = {1109500793},
issn = {07248741},
journal = {Pharmaceutical Research},
keywords = {Lung carcinoma cells,Paclitaxel,Poly(ethylene oxide),Poly($\epsilon$-caprolactone),Worm-like micelle},
number = {11},
