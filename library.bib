issn = {07349750},
journal = {Biotechnology Advances},
keywords = {Biological barrier,Gold nanomaterials,Nanomedicine,Nanoparticles toxicity,Physiochemical properties,Targeted drug delivery system,Therapeutic mechanism},
number = {5},
pages = {593--606},
publisher = {Elsevier Inc.},
title = {{Gold nanoparticles: Emerging paradigm for targeted drug delivery system}},
url = {http://dx.doi.org/10.1016/j.biotechadv.2012.10.002},
volume = {31},
year = {2013}
}
@article{Parker2013,
abstract = {The impact of NaCl and small hydrophobic molecules, used in perfumery, on the viscoelastic properties of aqueous solutions of sodium lauryl ether sulphate is studied. As the salt concentration increases, the viscosity passes through a maximum. Empirically, this behaviour is well known and is referred to as the 'salt curve'. Dynamic rheological measurements revealed a detailed picture of how salt affects the different length scales in the entangled wormlike micelles. Adding hydrophobic molecules does not change the shape of the salt curve, but they can shift the maximum to lower salt concentrations and alter the peak viscosity. The shift in the salt curve can generally be understood as due to either flattening of the interface, caused by insertion of amphiphilic molecules into the surfactant layer, or micellar swelling, caused by solubilization of very hydrophobic molecules in the micellar core. The viscosity decrease at the salt curve maximum is inversely proportional to the hydrophobicity of the added molecules, so less hydrophobic compounds cause larger decreases. It is hypothesized that these molecules interact with the surfactant headgroups and thereby soften the interfacial film. Our analysis suggests that the addition of hydrophobic additives reduces the persistence length and results in an effective shortening of the micelles. Introduction Giant wormlike micelles are elongated, semi-exible surfactant aggregates with remarkable rheological properties. Above a certain threshold concentration these surfactant solutions are viscoelastic due to the transient network formed by the entan-gled micelles. The viscoelasticity of giant micelles has been studied extensively, see for example ref. 1–5 and references therein. In contrast to polymers, wormlike micelles are dynamic systems that constantly break and recombine; therefore they are oen referred to as " living polymers " . 6,7 Understanding the viscoelasticity of wormlike micelles is very challenging as there are multiple pertinent length scales and stress relaxation mechanisms. Nevertheless, viscoelasticity gives a fascinating insight into the relationship between the molecular structure of such complex uids and their mechanical properties, and its understanding is important for the design and the development of industrial products containing giant wormlike micelles. These have many applications, including fracture uids in oil elds, drag reduction agents in hydrodynamic engineering and many home care, personal care and cosmetic products. 4,8 Here we have studied the effects of ionic strength and the addition of low concentrations of small hydrophobic molecules to aqueous solutions of the anionic surfactant sodium lauryl ether sulphate (SLES) (Scheme1). Entangled wormlike micelles of SLES have been shown to have viscoelastic properties. 9–11 It is one of the most widely used surfactants in consumer products for body care, such as shower gels, liquid hand soaps or shampoos. In the formulation of these products, salt (NaCl) is typically added to obtain a viscous gel. Increasing amounts of added salt signi-cantly affect the viscosity of the formulation, and the variation of the viscosity with the salt concentration is referred to as the 'salt curve'. 10–12 Typically, in the absence of salt the surfactant solutions are uid with viscosities close to that of water. The addition of a few weight percent of salt increases the zero-shear viscosity by several orders of magnitude up to a viscosity peak at a certain ionic strength. A further increase of salt decreases the viscosity. This variation is a convenient tool for formulators to ne tune the rheology of their products. Recently, Haward and McKinley demonstrated that the addition of salt can also have a dramatic effect on complex ows of surfactant solutions governed by viscoelasticity and shear thinning. 13 The molecular origin of these changes in viscosity is a constant transformation of the topology and the micellar shape as a function of ionic strength. The electrolyte screens the electrostatic repulsion between the charged headgroups of the surfactant, Scheme 1 Molecular structure of SLES. View Journal | View Issue decreasing the spontaneous curvature. 14,15 Typically, at a certain threshold concentration of salt, a transition from spherical to rodlike micelles occurs. Further addition of salt causes one-dimensional growth of the rods, resulting in the formation of giant, wormlike micelles that eventually overlap to form entangled networks. 16–19 At higher salt concentrations, the formation of three-way branches becomes energetically favourable. Branches were originally proposed by Porte et al. 20 Their existence was proved by cryo-TEM. 21,22 As Lequeux demonstrated, 23 branching causes a decrease in viscosity since branches allow micellar chains to slide along the micellar contour, thereby serving as stress relaxation points. Finally, at very high salt concentrations, the system phase separates into a concentrated, highly branched or liquid crystal phase and a dilute phase. The addition of small, hydrophobic molecules can also signicantly alter the rheological behaviour of surfactant systems. For example, adding perfume to a surfactant based formulation containing wormlike micelles, such as shower gels or shampoos, can lead to a thinning or a thickening of the base, to clouding or to phase separation, even at very low concentrations. A relationship between molecular struc-ture of perfume additives and micellar swelling in dilute surfac-tant systems has been obtained from studies with diffusion NMR spectroscopy. 24 Recently the formation of wormlike micelles in poly(oxyethylene) cholesteryl ether in aqueous solutions in the presence of lipophilic monoglycerides was shown to be induced by the addition of small amounts of aliphatic oils at low co-surfactant concentrations. 25 A dramatic inuence on the viscosity of wormlike micelles in aqueous solutions of CTAB was shown for trans-ortho-methoxycinnamic acid. 26 By irradiation with UV light the cis isomer was formed, which signicantly reduced the micellar length of the surfactant, thereby decreasing the zero-shear viscosity by four orders of magnitude. A particularly inter-esting observation has been reported by Munden in cosmetic formulations based on aqueous solutions of SLES. 12 The viscosity prole of the salt curve shied substantially in the presence of a hydrophobic additive, citronellol. With increasing concentrations of additive the viscosity peak shied to lower salt concentrations and the maximum steady shear viscosity was reduced. We study here the changes in the salt curve of SLES caused by the presence of small, hydrophobic molecules used as ingredients in perfumery. The focus is on linear viscoelasticity to provide an in-depth characterization of structural changes as a function of NaCl concentration and in the presence of the added molecules. Oscillatory rheology gives access to the various characteristic length scales and relaxation times that characterize the viscoelastic properties of these complex uids. We use the results to establish a relationship between the chemical nature of the hydrophobic additives and their effect on the macroscopic mechanical properties. Background},
author = {Parker, Alan and Fieber, Wolfgang},
doi = {10.1039/C2SM27078A},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Soft Matter/Parker, Fieber - 2013 - Viscoelasticity of anionic wormlike micelles effects of ionic strength and small hydrophobic molecules.pdf:pdf},
issn = {1744-683X},
journal = {Soft Matter},
number = {4},
pages = {1203--1213},
title = {{Viscoelasticity of anionic wormlike micelles: effects of ionic strength and small hydrophobic molecules}},
url = {http://xlink.rsc.org/?DOI=C2SM27078A},
volume = {9},
year = {2013}
}
