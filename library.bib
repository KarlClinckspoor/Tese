issn = {00219606},
journal = {Journal of Chemical Physics},
number = {13},
pages = {5559--5568},
title = {{Thermodynamics of anisotropic surfactant micelles. I. The influence of curvature free energy on the micellar size and shape}},
volume = {113},
year = {2000}
}
