doi = {10.1021/la015693r},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Langmuir/Arleth, Bergstr{\"{o}}m, Pedersen - 2002 - Small-angle neutron scattering study of the growth behavior, flexibility, and intermicellar interac.pdf:pdf;:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Langmuir/Arleth, Bergstr{\"{o}}m, Pedersen - 2002 - Small-angle neutron scattering study of the growth behavior, flexibility, and intermicellar inte(2).pdf:pdf;:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Langmuir/Arleth, Bergstr{\"{o}}m, Pedersen - 2002 - Small-angle neutron scattering study of the growth behavior, flexibility, and intermicellar inte(3).pdf:pdf},
