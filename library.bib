doi = {10.1021/la047954d},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Langmuir/Stodghill, Smith, O'Haver - 2004 - Thermodynamics of micellization and adsorption of three alkyltrimethylammonium bromides using isother.pdf:pdf},
