isbn = {1095-7103 (Electronic)$\backslash$r0021-9797 (Linking)},
issn = {00219797},
journal = {Journal of Colloid and Interface Science},
keywords = {1H NMR,Dynamic light-scattering,Ionic liquid mixture,Nonionic surfactant,Surfactant aggregation in ionic liquid mixture,Surfactant-ionic liquid interaction},
number = {1},
pages = {295--300},
pmid = {21767849},
publisher = {Elsevier Inc.},
title = {{Aggregation behavior of nonionic surfactants in ionic liquid mixtures}},
url = {http://dx.doi.org/10.1016/j.jcis.2011.06.052},
volume = {363},
year = {2011}
