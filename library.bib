@article{Fischer2002,
abstract = {Rheological investigations of viscoelastic surfactant solutions, both$\backslash$ntheoretical and experimental, have primarily focused on anionic or$\backslash$ncationic surfactant molecules and charged counterion pairs. In this$\backslash$narticle, we present an experimental investigation on the linear flow$\backslash$nproperties (stress relaxation) of uncharged aqueous dimer acid betaine$\backslash$nsolutions as a function of surfactant and salt concentration. In$\backslash$nthe zwitterionic dimer acid betaine solutions, the electrostatic$\backslash$ninteractions among the surfactant molecules are primarily not screened$\backslash$nby counterions. As a consequence, the theological properties are$\backslash$ndifferent from charged monomeric surfactant solutions. The analysis$\backslash$nof the theological properties of dimer acid betaine reveals that$\backslash$nthis surfactant system behaves differently from anionic or cationic$\backslash$ncompounds. Dimer acid betaine solutions with and without salt do$\backslash$nnot show monoexponential relaxation phenomena that are typical for$\backslash$nkinetically controlled relaxation mechanisms. Introducing NaCl also$\backslash$nleads to gel forming and an even higher stability of the aggregates.$\backslash$nAs a conclusion, the theological properties such as the zero shear$\backslash$nviscosity, relaxation times, of relaxation moduli are mainly controlled$\backslash$nby diffusion (reptation) processes. A model for the linear stress$\backslash$ndecay developed for ionic surfactants was used to characterize basic$\backslash$nstructural parameters. The aggregation of the surfactant and the$\backslash$nstructure of the supramolecular aggregates are, however, not completely$\backslash$ndecoded by the used model showing the complex nature of zwitterionic$\backslash$ndimeric surfactant systems.},
author = {Fischer, Peter and Rehage, Heinz and Gr{\"{u}}ning, Burghard},
doi = {10.1021/jp0127263},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Physical Chemistry B/Fischer, Rehage, Gr{\"{u}}ning - 2002 - Linear flow properties of dimer acid betaine solutions with and without changes ionic strength.pdf:pdf},
isbn = {1520-6106},
issn = {10895647},
journal = {Journal of Physical Chemistry B},
number = {42},
pages = {11041--11046},
title = {{Linear flow properties of dimer acid betaine solutions with and without changes ionic strength}},
volume = {106},
year = {2002}
}
