doi = {10.1016/0021-9797(88)90377-3},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Colloid And Interface Science/Candau et al. - 1988 - Network properties of semidilute aqueous KBr solutions of cetyltrimethylammonium bromide.pdf:pdf},
isbn = {0021-9797},
