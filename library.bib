arxivId = {arXiv:1011.1669v3},
author = {Shiraishi, Yasuhiro and Inoue, Takuya and Hirai, Takayuki},
doi = {10.1021/la1028993},
eprint = {arXiv:1011.1669v3},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Langmuir/Shiraishi, Inoue, Hirai - 2010 - Local viscosity analysis of triblock copolymer micelle with cyanine dyes as a fluorescent probe.pdf:pdf},
isbn = {9788578110796},
issn = {07437463},
journal = {Langmuir},
number = {22},
pages = {17505--17512},
pmid = {20942435},
title = {{Local viscosity analysis of triblock copolymer micelle with cyanine dyes as a fluorescent probe}},
volume = {26},
year = {2010}
}
@article{Jaeger2009a,
abstract = {This paper presents an optical approach, based on the use of a low-cost charge-coupled device (CCD) camera, for the quantitative determination of solute concentrations in saturated porous media. The method is applied to evaluate tracer experiments carried out in a laboratory model tank. The CCD photos deliver RGB values which are transferred into concentrations for the evaluation of vertical concentration profiles over the whole tank area. A specially developed evaluation procedure, including internal referencing for noise reduction, considers the colour of the adjacencies of the evaluated spots and scattering effects. The CCD data evaluation technique is accompanied by conventional sampling and absorption measurements and by numerical flow and transport simulations. This non-invasive technique allows a direct mapping of the concentration distribution without any disturbance of the solute plume. Therefore, it turns out to be an important tool for a detailed investigation of fundamental processes (e.g. transverse dispersion) determining the solute (e.g. contaminant) transport in porous media.},
author = {Jaeger, Stefanie and Ehni, Markus and Eberhardt, Christina and Rolle, Massimo and Grathwohl, Peter and Gauglitz, Guenter},
doi = {10.1007/s00216-009-2978-3},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Analytical and Bioanalytical Chemistry/Jaeger et al. - 2009 - CCD camera image analysis for mapping solute concentrations in saturated porous media.pdf:pdf},
isbn = {0021600929},
issn = {16182642},
journal = {Analytical and Bioanalytical Chemistry},
keywords = {Colour tracer,Imaging,Numerical modelling,Optical sensor,Porous media},
number = {6},
pages = {1867--1876},
pmid = {19649770},
title = {{CCD camera image analysis for mapping solute concentrations in saturated porous media}},
volume = {395},
year = {2009}
}
@article{Shchipunov1999,
abstract = {The effect of an external electric field on ternary mixtures consisting of lecithin, n-decane, and small amounts of polar additives (water or glycerol) has been studied by oscillating theology, polarizing microscopy, and electric birefringence. It is shown that an electric field that is applied induces a so-called electrotheological (ER) effect, i.e., an increase in the viscosity and dynamic shear moduli of all the examined mixtures. The ER effect is absent in solutions of nonoverlapping micelles. The electric field causes the formation of fiber-like structures in the interelectrode gap. The ER effect becomes evident at a critical field of about 40 and 100 V/mm for water- and glycerol-containing organogels, respectively. In the latter case, a region of the reproducible and stable ER effect is extended up to 1700 V/mm, which is 3-4 times greater than that observed in the jelly-like phases with water. The buildup, as followed from birefringent measurements, includes fast and slow processes. Those correspond to both the local motions of parts of micellar chains and the restructuring of the whole network under the action of an external electric field. The ER response depends on the molar ratio of the polar additives to lecithin. Diagrams describing the behavior of ternary mixtures under the electric field have been constructed. They differ for water- and glycerol-containing organogels. The dependence of the stable ER effects on the molar ratio of glycerol to lecithin has a maximum in the vicinity of the phase separation of the homogeneous organogel, whereas for water-containing systems there is a gradual increase up to and including mixtures with the solid precipitate. A new theological regime has been first established for solutions of polymer-like micelles. This feature is the square-root scaling of the dynamic moduli with the frequency. Such a scaling is inherent in polymers. A possible mechanism is considered, basing on the ordering of cylindrical micelles under the action of an external electric field.},
author = {Shchipunov, Yu A. and D{\"{u}}rrschmidt, T. and Hoffmann, H.},
doi = {10.1006/jcis.1998.6046},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Colloid and Interface Science/Shchipunov, D{\"{u}}rrschmidt, Hoffmann - 1999 - Electrorheological effects in lecithin organogels with water and glycerol.pdf:pdf},
issn = {00219797},
journal = {Journal of Colloid and Interface Science},
keywords = {Electrorheological effect,Lecithin,Organogel,Polymer-like micelles},
number = {2},
pages = {390--401},
pmid = {10092369},
title = {{Electrorheological effects in lecithin organogels with water and glycerol}},
volume = {212},
year = {1999}
}
@article{Glindemann2006a,
abstract = {The smell of money and chem. weapons: The perplexing metallic odor from touching iron tools or coins is a type of human body odor linked to the decompn. of skin peroxides. Fe2+ ion contg. rust, drinking water, and blood all cause a similar metallic odor. Another "garlic" metallic odor, that of phosphorus-alloyed iron under acid attack, is caused by organophosphines, including C-P compds. which are monitored under the Chem. Weapons Convention. [on SciFinder(R)]},
