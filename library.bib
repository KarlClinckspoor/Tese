@article{Paul2007,
abstract = {The structural and energetic properties of solutions containing water, urea, and thmethylamine-N-oxide (TMAO) are examined using molecular dynamics simulations. Such systems are of interest mainly because TMAO acts to counter the protein denaturing effect of urea. Even at relatively high concentration, TMAO is found to fit well into the urea-water structure. The underlying solution structure is influenced by TMAO, but these perturbations tend to be modest. The TMAO-water and TMAO-urea interaction energies make an important contribution to the total energy in solutions where counter-denaturing effects are expected. TMAO-water and TMAO-urea hydrogen bonds have the largest hydrogen-bond energies in the system. Additionally, TMAO cannot hydrogen bond with itself, and hence it interacts strongly with water and urea. These observations suggest that the mechanism of TMAO counter denaturation is simply that water and urea prefer to solvate TMAO rather than the protein, hence inhibiting its unfolding. {\textcopyright} 2007 American Chemical Society.},
author = {Paul, Sandip and Patey, Grenfell N.},
doi = {10.1021/ja0685506},
