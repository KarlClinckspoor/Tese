doi = {10.1021/la404692u},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Langmuir/Bickel et al. - 2014 - Microcalorimetric determination of the entropy change upon the electrochemically driven surface aggregation of do.pdf:pdf},
