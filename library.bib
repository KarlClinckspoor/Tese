@article{Hartmann1997,
abstract = {We have experimentally studied the rheological behaviour of an aqueous cetyltrimethylammonium bromide (CTAB) solution with sodium salicylate (NaSal) as added salt. The CTAB concentration C(D) was fixed at 0.1 M, and in this work we observe the evolution of the linear and non-linear rheological behaviour of the solution when the salt concentration C(S) is below or equal to C(D), varying between 0.015 and 0.1 M. We observe two very different kinds of behaviours. For the low C(S) concentration, we can obtain a shear-thickening effect. The critical shear rate at which this occurs is a function of the temperature T, and C(S). We give here the evolution of this critical parameter. When C(S) increases and reaches a limit value, this behaviour disappears and we obtain a shear-thinning effect with a shear stress plateau for a certain range of shear rate. In the plateau domain an induced shear-banding structure must exist: bands of high and low shear rate co-exist under flow.},
author = {Hartmann, V. and Cressely, R.},
doi = {10.1016/S0927-7757(96)03773-9},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Colloids and Surfaces A Physicochemical and Engineering Aspects/Hartmann, Cressely - 1997 - Influence of sodium salicylate on the rheological behaviour of an aqueous CTAB solution.pdf:pdf},
issn = {09277757},
journal = {Colloids and Surfaces A: Physicochemical and Engineering Aspects},
keywords = {cetyltrimethylammonium bromide (CTAB),micelles,non-Newtonian viscosity,shear-banding,shear-thickening,sodium salicylate (NaSal)},
number = {2-3},
pages = {151--162},
title = {{Influence of sodium salicylate on the rheological behaviour of an aqueous CTAB solution}},
volume = {121},
year = {1997}
}
