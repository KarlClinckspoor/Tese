isbn = {0021-9584},
issn = {0021-9584},
journal = {Journal of Chemical Education},
keywords = {Applications of Chemistry,Dyes / Pigments,General Public,History / Philosophy,Interdisciplinary / Multidisciplinary},
number = {4},
