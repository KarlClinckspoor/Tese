doi = {10.1021/j150646a029},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Physical Chemistry/Corti, Minero, Degiorgio - 1984 - Cloud point transition in nonionic micellar solutions.pdf:pdf},
