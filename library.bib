issn = {00442968},
journal = {Zeitschrift fur Kristallographie - New Crystal Structures},
number = {3-4},
pages = {237--254},
title = {{Strukturverfeinerung von Harnstoff mit Neutronenbeugungsdaten bei 60, 123 und 293 K und X-N- und X-X(ls2)-Synthesen bei etwa 100 K}},
volume = {153},
