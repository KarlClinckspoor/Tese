 number = {10},
 pages = {907-917},
 title = {1,3-Butanediol as a co-solvent for the surfactant solutions},
 volume = {290},
 year = {2012},
 source = {Crossref},
 url = {https://doi.org/10.1007/s00396-012-2603-4},
