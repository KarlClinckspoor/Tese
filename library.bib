@article{Norenzayan2013a,
abstract = {Although most people are religious, there are hundreds of millions of religious disbelievers in the world. What is religious disbelief and how does it arise? Recent developments in the scientific study of religious beliefs and behaviors point to the conclusion that religious disbelief arises from multiple interacting pathways, traceable to cognitive, motivational, and cultural learning mechanisms. We identify four such pathways, leading to four distinct forms of atheism, which we term mindblind atheism, apatheism, inCREDulous atheism, and analytic atheism. Religious belief and disbelief share the same underlying pathways and can be explained within a single evolutionary framework that is grounded in both genetic and cultural evolution. {\textcopyright} 2012 Elsevier Ltd.},
