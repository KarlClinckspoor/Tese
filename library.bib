file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Colloid and Interface Science/Rodrigues, Ito, Sabadini - 2011 - Thermal-stability of mixed giant micelles of alkyltrimethylammonium surfactants and salicylate.pdf:pdf},
issn = {00219797},
