journal = {Soft Matter},
number = {42},
pages = {10941},
title = {{Impact of branching on the viscoelasticity of wormlike reverse micelles}},
url = {http://xlink.rsc.org/?DOI=c2sm26528a},
volume = {8},
year = {2012}
}
@article{Percebom2018,
