@article{Naylor1995,
abstract = {The dynamics of crystallization-driven microphase separation in semicrystalline block copolymers are investigated using diblocks of ethylene-(ethylenedt-propylene) (E/EP) and ethylene- ethylethylene (E/EE). Simultaneous, time-resolved small-angle and wide-angle X-ray scattering (SAXS and WAXS) on heating and cooling are used to study the formation of the microphase-separated lamellar structure and the crystallization of the ethylene blocks, respectively. Sharp first- and higher-order SAXS peaks (as many as four reflections) evolve rapidly and simultaneously on cooling from the homogeneous melt, characteristic of a highly-ordered domain structure with relatively sharp interfaces. These semicrystalline diblocks thus form microdomains which appear strongly segregated, even though the small undercoolings used would put amorphous block copolymers in the weak segregation regime. Crystallization of the E blocks exactly tracks the development of the microdomain structure. During the initial rapid structure development, the SAXS peak positions are constant, suggesting that the ordered structures nucleate and simply grow to fill the sample, without any accompanying internal rearrangement. At longer times, a small decrease of the microdomain spacing is observed, attributed to the relaxation of a structure initially out of equilibrium.},
author = {Naylor, Steven and Ryan, Anthony J and Bras, Wim and Adamson, D H and Fetters, L. J. and Rangardan, Pratima and Register, Richard},
doi = {10.1021/ma00109a013},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Macromolecules/Naylor et al. - 1995 - Dynamics of Structure Formation in Crystallizable Block Copolymers.pdf:pdf},
issn = {0024-9297},
journal = {Macromolecules},
number = {5},
pages = {1422--1428},
title = {{Dynamics of Structure Formation in Crystallizable Block Copolymers}},
volume = {28},
year = {1995}
}
@article{Kang2017a,
abstract = {Responsive wormlike micelles are very useful in a number of applications, whereas it is still challenging to create dramatic viscosity changes in wormlike micellar systems. Here we developed a pH-responsive wormlike micelles based on noncovalent constructed surfactant, which is formed by complexation of N-erucamidopropyl-N,N-dimethylamine (UC22AMPM) and citric acid at the molar ratio of 3:1 (EACA). The phase behavior, aggregate microstructure and viscoealsticity of EACA solutions were investigated by macroscopic appearance observation, rheological and Cryo-TEM measurements. It was found that the phase behavior of EACA solutions undergoes transitions from transparent viscoelastic fluid to opalescent solution and then phase separation with white floater upon increasing pH. With increasing pH from 2.03 to 6.17, the viscosity of wormlike micelles in the transparent solutions continously increased and reached to {\~{}}683,000 mPa•s at pH 6.17. As pH was adjusted to 7.13, the opalescent solution shows a water-like flowing behaviour and the $\eta$0 rapid declines to {\~{}}1 mPa•s. Thus, dramatic viscosity changes of about 6 magnitutes can be triggered by varying pH without any deterioration for EACA system. This drastic variation in rheological behavior is attributed to the pH dependent interaction between UC22AMPM and citric acid. Furthermore, concentration and temperature dependence of the rheological behavior for EACA solutions were also studied to benefit for obtaining a desired pH-responsive viscosity changes.},
author = {Kang, Wanli and Wang, Pengxiang and Fan, Haiming and Yang, Hongbin and Dai, Caili and Yin, Xia and Zhao, Yilu and Guo, Shujun},
doi = {10.1039/C6SM02655A},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Soft Matter/Kang et al. - 2017 - A pH-responsive wormlike micellar system of a noncovalent interaction-based surfactant with a tunable molecular str.pdf:pdf},
issn = {1744-683X},
journal = {Soft Matter},
number = {6},
pages = {1182--1189},
publisher = {Royal Society of Chemistry},
title = {{A pH-responsive wormlike micellar system of a noncovalent interaction-based surfactant with a tunable molecular structure}},
url = {http://xlink.rsc.org/?DOI=C6SM02655A},
volume = {13},
year = {2017}
}
