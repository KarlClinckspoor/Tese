doi = {10.1016/0378-5173(82)90143-0},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/International Journal of Pharmaceutics/Badwan et al. - 1982 - The solubility of benzodiazepines in sodium salicylate solution and a proposed mechanism for hydrotropic solubili.pdf:pdf},
