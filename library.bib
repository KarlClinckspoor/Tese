@article{Nagarajan1985a,
abstract = {A mol. theory for the formation of mixed micelles is developed as an extension of an earlier theory for single-component micelles. The theory permits the calcn. of the cmc, the av. micelle size, and the av. micelle compn. as well as the size and the compn. distribution of micelles in mixed surfactant systems. Central to the theory is an explicit mol. thermodn. expression for the Gibbs energy of formation of the mixed micelle. This Gibbs energy of micelle formation, which is compn. dependent, can be decompd. into bulk and interfacial components. The bulk component includes the free energy contributions arising from the transfer of the surfactant tail from H2O to the micelle. The interfacial component includes the free energy contributions due to the micellar core-H2O interfacial energy, the steric repulsions between the head groups at the micelle surface, and the electrostatic repulsions between the head groups at the micelle surface. If the Gibbs energy of micelle formation is a linear function of the micelle compn., then the excess Gibbs energy is 0 and the surfactant mixt. behaves ideally. On the other hand, if the Gibbs energy of micelle formation has a nonlinear dependence on the micelle compn., then the excess Gibbs energy is nonzero and the surfactant mixt. displays nonideal micellization behavior. It is shown that the bulk component of the Gibbs energy of micelle formation deends linearly on the micelle compn. and hence does not account for any nonideal behavior displayed by the mixed micelles. In contrast, the interfacial component has a nonlinear dependence on the compn. of the mixed micelle and thus constitutes the source of the obsd. nonidealities in mixed micellar systems. Therefore, surfactant mixts. displaying markedly nonideal micellization behavior are those for which the interfacial interactions show significant compn. dependence. Illustrative results based on this mol. theory are obtained for a no. of binary mixts. and are compared against the available exptl. data.},
author = {Nagarajan, R.},
doi = {10.1021/la00063a012},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Langmuir/Nagarajan - 1985 - Molecular Theory for Mixed Micelles.pdf:pdf},
isbn = {0743-7463},
issn = {15205827},
journal = {Langmuir},
number = {3},
pages = {331--341},
title = {{Molecular Theory for Mixed Micelles}},
volume = {1},
year = {1985}
}
