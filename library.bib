journal = {Langmuir},
number = {1},
pages = {140--148},
pmid = {15620295},
title = {{Biocompatible lecithin organogels: Structure and phase equilibria}},
volume = {21},
year = {2005}
}
@article{Bergstrom2000,
abstract = {The structural behavior of surfactant micelles is studied. The micelles are considered as generally shaped as triaxial tablets with distinct thickness, width and length and they become spherical, spherocylindrical or disk-shaped depending on the dimensions. The micelle size is found to depend mainly on a constant k 1 , whereas another constant k 2  influences the shape. The two constants are related to the first and second order correction in curvature of the micellar end caps. Large positive k 2  values promote the formation of tablets rather than very long spherocylinders. An additional parameter related to the curvature of the straight cylindrical rims may influence the structure of the tablet-shaped micelles insofar k 2  is close to or below zero.},
