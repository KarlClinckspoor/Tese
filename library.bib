@article{Ganachaud2005,
abstract = {The preparation of polymeric particles and capsules by means of spontaneous droplet formation and subsequent polymer precipitation or synthesis is well-known. However, spontaneous emulsification is a phenomenon that has often been erroneously interpreted. This Minireview provides new insights into the preparation of metastable liquid dispersions by homogeneous liquid-liquid nucleation, and is based primarily on a recent study by Vitale and Katz (Langmuir, 2003, 19, 4105-4110). This spontaneous emulsification, which they named the Ouzo effect, occurs upon pouring, into water, a mixture of a totally water-miscible solvent and a hydrophobic oil--and optionally some water--thus generating long-lived small droplets, which are formed even though no surfactant is present. Herein, we review and reinterpret the most relevant publications on the synthesis of a variety of dispersions (pseudolatexes, silicone emulsions, biodegradable polymeric nanocapsules, etc.), which we believe have actually been synthesized using the Ouzo effect. The Ouzo effect may also become a substitute for high-shear techniques, which, to date have only been of limited utility on industrial scales.},
author = {Ganachaud, Fran{\c{c}}ois and Katz, Joseph L.},
doi = {10.1002/cphc.200400527},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/ChemPhysChem/Ganachaud, Katz - 2005 - Nanoparticles and nanocapsules created using the ouzo effect Spontaneous emulsification as an alternative to ul.pdf:pdf},
isbn = {1439-7641},
issn = {14394235},
journal = {ChemPhysChem},
keywords = {Emulsions,Interfaces,Nanoparticles,Polymerization,Spontaneous emulsification},
number = {2},
pages = {209--216},
pmid = {15751338},
title = {{Nanoparticles and nanocapsules created using the ouzo effect: Spontaneous emulsification as an alternative to ultrasonic and high-shear devices}},
volume = {6},
year = {2005}
}
@article{Ikeda2017,
abstract = {In order to understand the non-Arrhenius structural relaxation in a systematic way, a measure of cooperativity is employed, which is based on the bond strength–coordination number fluctuation model developed by the authors. In our model, the cooperativity is described by the temperature-dependent number of structural units NBinvolved in the structural relaxation, which increases from unity at the high temperature limit up to of the order of 100 near the glass transition temperature Tg. This is comparable to that estimated by the Donth formula, but is much larger than that obtained from the Adam–Gibbs model. At higher temperature region a characteristic temperature Txis extracted from the cooperativity analysis, which could be associated with the transition temperature TAacross which the relaxation behavior changes from Arrhenius-like to non-Arrhenius-like. In the polymer melts and molecular liquids investigated, the characteristic temperature ratio Tx/Tgis found to be 1.67–1.81, which agrees with the typical TA/Tgvalues reported. The extracted temperature Txenables the characterization of the high-temperature relaxation behavior in a new way that differs from other methods. A possible interpretation of TAis given in terms of the cooperativity onset.},
author = {Ikeda, Masahiro and Aniya, Masaru},
doi = {10.1016/j.eurpolymj.2016.11.005},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/European Polymer Journal/Ikeda, Aniya - 2017 - A measure of cooperativity in non-Arrhenius structural relaxation in terms of the bond strength–coordination numbe.pdf:pdf},
isbn = {0022-3093},
issn = {00143057},
journal = {European Polymer Journal},
keywords = {Cooperativity,Glass transition,Non-Arrhenius relaxation},
pages = {29--40},
publisher = {Elsevier B.V.},
title = {{A measure of cooperativity in non-Arrhenius structural relaxation in terms of the bond strength–coordination number fluctuation model}},
