journal = {Langmuir},
number = {38},
pages = {10524--10531},
pmid = {26348207},
title = {{Mechanism of Nanorod Formation by Wormlike Micelle-Assisted Assembly of Nanospheres}},
volume = {31},
year = {2015}
}
