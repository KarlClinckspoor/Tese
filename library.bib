file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Biochemical and Biophysical Methods/Briggner, Wads{\"{o}} - 1991 - Test and calibration processes for microcalorimeters, with special reference to heat conduction instruments use.pdf:pdf},
issn = {0165022X},
journal = {Journal of Biochemical and Biophysical Methods},
keywords = {Calibration,Calorimeter,Reaction,Test process},
number = {2},
pages = {101--118},
pmid = {2061558},
title = {{Test and calibration processes for microcalorimeters, with special reference to heat conduction instruments used with aqueous systems}},
