journal = {Colloids and Surfaces B: Biointerfaces},
keywords = {Bicellar systems,Cryo-transmission electron microscopy,Diclofenac,Dihexanoyl-glycero-phosphatidylcholine,Dimyristoyl-glycero-phosphatidylcholine,Dipalmitoyl-glycero-phosphatidylcholine,Flufenamic acid,In vitro percutaneous absorption},
pages = {322--326},
