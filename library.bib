@article{Sarac2011,
abstract = {The complex aggregation processes of dodecyltrimethylammonium chloride (DTAC) have been studied in dilute solutions of sodium salicylate (NaSal) by isothermal titration calorimetry and electrical conductivity at temperatures between 278.15 K and 318.15 K. A structural transformation that was dependent on the concentrations of DTAC and NaSal was observed. The micellization process in dilute solutions of DTAC has been subjected to a detailed thermodynamic analysis and shown to occur at considerably lower critical micelle concentrations than reported for DTAC in water and NaCl solutions. Gibbs free energy, $\Delta$ mic G o, and entropy, $\Delta$ mic S o, were deduced by taking into account the degree of micelle ionization, $\beta$, estimated from conductivity measurements. From the temperature dependence of the enthalpy of micellization, $\Delta$ mic H o, the heat capacities of micellization, $\Delta$micc o p were determined and discussed in terms of the removal of large areas of non-polar surface from contact with water upon micellization. The process is exothermic at all temperatures, indicating, in addition to the hydrophobic effect, the presence of strong interactions between surfactant and salicylate ions. These were confirmed by 1H NMR spectroscopy and diffusion NMR experiments. Salicylate ions not only interact with the headgroups but also insert further into the micelle core. At c NaSal/c DTAC {\textgreater} 2.5, the structural rearrangements occur even at relatively low concentrations of NaSal.},
author = {{\v{S}}arac, Bojan and Cerkovnik, Janez and Ancian, Bernard and M{\'{e}}riguet, Guillaume and Roger, Ga{\"{e}}lle M. and Durand-Vidal, Serge and Be{\v{s}}ter-Roga{\v{c}}, Marija},
doi = {10.1007/s00396-011-2480-2},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Colloid and Polymer Science/{\v{S}}arac et al. - 2011 - Thermodynamic and NMR study of aggregation of dodecyltrimethylammonium chloride in aqueous sodium salicylate solut.pdf:pdf},
issn = {0303402X},
journal = {Colloid and Polymer Science},
keywords = {Dodecyltrimethylammonium chloride,Electrical conductivity,Isothermal titration calorimetry,Micellization,NMR self-diffusion,Sodium salicylate,Thermodynamics},
number = {14},
pages = {1597--1607},
title = {{Thermodynamic and NMR study of aggregation of dodecyltrimethylammonium chloride in aqueous sodium salicylate solution}},
volume = {289},
year = {2011}
}
