doi = {10.1122/1.1332387},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Rheology/Thiele et al. - 2001 - Rheology and nuclear magnetic resonance measurements under shear of sodium dodecyl sulfatedecanolwater nematics.pdf:pdf},
