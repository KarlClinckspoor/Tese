isbn = {0022-3654$\backslash$r1541-5740},
issn = {0022-3654},
journal = {J. Phys. Chem.},
number = {9},
pages = {1044--1057},
title = {{Thermodynamic analysis of the growth of sodium dodecyl sulfate micelles}},
url = {http://pubs.acs.org/doi/abs/10.1021/j100446a021},
volume = {84},
year = {1980}
}
@article{Nagornyak2009,
abstract = {Although it has been long known that like-charged particles attract$\backslash$none another in aqueous media, the mechanism underlying this counter-intuitive$\backslash$nphenomenon has remained controversial. We tested the hypothesis put$\backslash$nforth long ago by Langmuir and again by Feynman and by Ise, that$\backslash$nthe attraction between like-charged entities lies in an intermediate$\backslash$nof unlike charges. Tests were facilitated by the observation that$\backslash$nthe attractive forces could be confirmed between widely separated$\backslash$nparticles of macroscopic size. Two approaches showed comparable results.$\backslash$nIn the first, pH-sensitive dyes showed intermediate zones of opposite$\backslash$ncharge: an accumulation of protons was found between negatively charged$\backslash$nspheres, whereas between positively charged spheres the intermediate$\backslash$nzone contained OH- groups. In the second and complementary approach,$\backslash$nmicroelectrode measurements showed that in between negatively charged$\backslash$nspheres, the electrical potential was relatively positive, whereas$\backslash$nbetween positively charged spheres it was relatively negative. Hence,$\backslash$nboth approaches confirm theoretical expectations. The large number$\backslash$nof unlike charges lying in between the like-charged spheres may come$\backslash$nfrom the build-up of the recently reported "exclusion zone" surrounding$\backslash$neach particle.},
