 file = {:C\:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/The Journal of Chemical Physics/De Gennes - 1971 - Reptation of a polymer chain in the presence of fixed obstacles.pdf:pdf},
 isbn = {00219606},
