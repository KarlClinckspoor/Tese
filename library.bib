doi = {10.1021/jp500295e},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Physical Chemistry B/Liger-Belair - 2014 - How many bubbles in your glass of bubbly.pdf:pdf},
