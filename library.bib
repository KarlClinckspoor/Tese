file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Soft Matter/Padoan et al. - 2013 - Behavior of wormlike micellar solutions formed without any additives from semi-fluorinated quaternary ammonium sa.pdf:pdf},
issn = {1744-683X},
journal = {Soft Matter},
number = {37},
pages = {8992},
title = {{Behavior of wormlike micellar solutions formed without any additives from semi-fluorinated quaternary ammonium salts}},
url = {http://xlink.rsc.org/?DOI=c3sm51591e},
volume = {9},
year = {2013}
}
