file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/J. Phys. Chem. B/Hedin, Sitnikov - 1999 - Shape Changes of C 16 TABr Micelles on Benzene Solubilization.pdf:pdf},
