file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Polyhedron/Abbehausen et al. - 2011 - Synthesis, spectroscopic characterization, DFT studies and biological assays of a novel gold(I) complex with.pdf:pdf},
issn = {02775387},
journal = {Polyhedron},
keywords = {2-Mercaptothiazoline,Antibacterial activity,Cytotoxic assays,Gold(I),Infrared spectroscopy,NMR spectroscopy},
number = {13},
