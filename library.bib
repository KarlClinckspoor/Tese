issn = {18734359},
journal = {Colloids and Surfaces A: Physicochemical and Engineering Aspects},
keywords = {Capillary pressure tensiometry,Dilational visco-elasticity,Drop formation and detachment,Dynamic interfacial tension,High frequency oscillations,Oscillating drop and bubble,Short adsorption times},
pages = {159--168},
publisher = {Elsevier B.V.},
title = {{Fast dynamic interfacial tension measurements and dilational rheology of interfacial layers by using the capillary pressure technique}},
