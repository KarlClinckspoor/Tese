isbn = {00219606},
issn = {00219797},
journal = {Journal of Colloid and Interface Science},
number = {1},
pages = {181--193},
pmid = {16061239},
title = {{Model calculations of the spontaneous curvature, mean and Gaussian bending constants for a thermodynamically open surfactant film}},
volume = {293},
year = {2006}
}
