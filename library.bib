@article{Vitale2003,
abstract = {The "ouzo effect" enables one to create a dispersion of small droplets$\backslash$nin a surrounding liquid phase without the use of surfactants, dispersing$\backslash$nagents, or mechanical agitation: a phenomenon which can be of value$\backslash$nin many disciplines. In the quantitative studies presented here,$\backslash$ndispersions of oil droplets in water are formed by the addition of$\backslash$nwater to a solution of the oil dissolved in a solvent. This causes$\backslash$nthe oil to supersaturate and then nucleate into small droplets. The$\backslash$nmean droplet diameter is a function only of the oil-to-solvent ratio$\backslash$nat a given temperature. The number density of droplets formed can$\backslash$nbe controlled independently from the droplet diameter by changing$\backslash$nthe amount of water added. Smaller droplets are formed by using more$\backslash$nhydrophilic cosolvents. The droplet size distribution is typically$\backslash$nlog-normal. The width of the distribution can be narrowed by mixing$\backslash$nthe components at an elevated temperature and then allowing the dispersion$\backslash$nto cool.},
author = {Vitale, Steven A. and Katz, Joseph L.},
doi = {10.1021/la026842o},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Langmuir/Vitale, Katz - 2003 - Liquid droplet dispersions formed by homogeneous liquid-liquid nucleation The ouzo effect.pdf:pdf},
isbn = {0743-7463},
issn = {07437463},
journal = {Langmuir},
number = {10},
pages = {4105--4110},
title = {{Liquid droplet dispersions formed by homogeneous liquid-liquid nucleation: "The ouzo effect"}},
volume = {19},
year = {2003}
}
