keywords = {DLS,Detergent,Diphenylhexatriene,Isothermal titration calorimetry,Liposome,Osmolyte},
number = {1-3},
pages = {119--128},
pmid = {20417021},
publisher = {Elsevier B.V.},
title = {{Effects of glycerol and urea on micellization, membrane partitioning and solubilization by a non-ionic surfactant}},
