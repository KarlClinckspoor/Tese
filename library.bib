file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Langmuir the ACS journal of surfaces and colloids/Croce et al. - 2004 - Impacting the length of wormlike micelles using mixed surfactant systems.pdf:pdf},
issn = {0743-7463},
journal = {Langmuir : the ACS journal of surfaces and colloids},
keywords = {Alcohols,Alcohols: chemistry,Cryoelectron Microscopy,Cryoelectron Microscopy: methods,Methylamines,Methylamines: chemistry,Micelles,Neutron Diffraction,Neutron Diffraction: methods,Particle Size,Sensitivity and Specificity,Surface Properties,Surface-Active Agents,Surface-Active Agents: chemistry,Viscosity},
number = {19},
pages = {7984--90},
pmid = {15350062},
title = {{Impacting the length of wormlike micelles using mixed surfactant systems.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15350062},
volume = {20},
year = {2004}
}
@article{Penfold2002a,
abstract = {Small angle neutron scattering, SANS, has been used to investigate the structure of mixed ionic-nonionic surfactant micelles of SDS and C12E6. For solution compositions rich in C12E6, the evolution of micelle size with composition exhibits a pronounced maximum, in qualitative, agreement with the theoretical predictions of Shiloach and Blankschtein [Langmuir 1998, 14, 7166], but with some important quantitative differences which are discussed. The solubilization of the alkane, hexadecane, into the mixed surfactant micelle suppresses the micelle growth with changing solution composition. For micelles rich in C12E6, hexadecane is solubilized predominantly into the palisade layer of the micelles, favoring small spherical micelles. The micellar growth with changing solution composition arises from changes in the balance between the headgroup and steric contributions to the free energy of micellization, and the addition of alkane further alters that balance in favor of small spherical micelles.},
author = {Penfold, J. and Staples, E. and Tucker, I.},
doi = {10.1021/jp013810l},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Physical Chemistry B/Penfold, Staples, Tucker - 2002 - Neutron small angle scattering studies of micellar growth in mixed anionic-nonionic surfactants, sodiu.pdf:pdf},
isbn = {1520-6106},
issn = {10895647},
journal = {Journal of Physical Chemistry B},
number = {34},
pages = {8891--8897},
title = {{Neutron small angle scattering studies of micellar growth in mixed anionic-nonionic surfactants, sodium dodecyl sulfate, SDS, and hexaethylene glycol monododecyl ether, C12E6, in the presence and absence of solubilized alkane, hexadecane}},
volume = {106},
year = {2002}
}
@article{Ober2011a,
abstract = {In this study, we use microparticle image velocimetry ($\mu$-PIV) and adapt a commercial birefringence microscopy system for making full-field, quantitative measurements of flow-induced birefringence(FIB) for the purpose of microfluidic, optical rheometry of two wormlike micellar solutions. In combination with conventional rheometric techniques, we use our microfluidic rheometer to study the properties of a shear-banding solution of cetylpyridinium chloride (CPyCl) with sodium salicylate (NaSal) and a nominally shear-thinningsystem of cetyltrimethylammonium bromide (CTAB) with NaSal across many orders of magnitude of deformation rates ( 10 - 2 ≤ $\gamma$ {\textperiodcentered} ≤ 10 4 s - 1 ). We use $\mu$-PIV to quantify the local kinematics and use the birefringence microscopy system in order to obtain high-resolution measurements of the changes in molecular orientation in the wormlike fluids under strong deformations in a microchannel. The FIBmeasurements reveal that the CPyCl system exhibits regions of localized, high optical anisotropy indicative of shear bands near the channel walls, whereas the birefringence in the shear-thinning CTAB system varies more smoothly across the width of the channel as the volumetric flow rate is increased. We compare the experimental results to the predictions of a simple constitutive model, and we document the breakdown in the stress-optical rule as the characteristic rate of deformation is increased.},
author = {Ober, Thomas J. and Soulages, Johannes and McKinley, Gareth H.},
doi = {10.1122/1.3606593},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Rheology/Ober, Soulages, McKinley - 2011 - Spatially resolved quantitative rheo-optics of complex fluids in a microfluidic device.pdf:pdf},
issn = {0148-6055},
journal = {Journal of Rheology},
number = {5},
pages = {1127--1159},
title = {{Spatially resolved quantitative rheo-optics of complex fluids in a microfluidic device}},
url = {http://sor.scitation.org/doi/10.1122/1.3606593},
volume = {55},
year = {2011}
}
@article{Engelskirchen2006,
abstract = {With successive addition of mixed polyoxyethylene dodecylether (C12EOn) surfactant with an average EO number of 3 to the aqueous solution of hexadecyltrimethylammonium bromide (CTAB) and sucrose monohexadecanoate (P1695), one-dimensional micellar growth occurs and the viscosity increases sharply leading to the formation of viscoelastic solutions of worm-like micelles. Steady and oscillatory shear rheological measurements show that in the aqueous CTAB system, the extent of micellar growth decreases with increasing the dispersion in EO chain in the mixture in following order C12EO3{\textgreater} C12EO2+ C12EO4{\textgreater} C12EO0+ C12EO4{\textgreater} C12EO1+ C12EO5. With C12EO0+ C12EO6no micellar growth occurs but a phase separation is observed. In P1695 system, the viscoelastic solution is formed at relatively lower concentration of mixed C12EOnand the extent of micellar growth decreases in the following order: C12EO3≈ C12EO2+ C12EO4{\textgreater} C12EO1+ C12EO5{\textgreater} C12EO0+ C12EO6. The effect of the dispersion of EO chain length on the micellar growth is also discussed. {\textcopyright} 2006 Elsevier B.V. All rights reserved.},
author = {Engelskirchen, Sandra and Acharya, Durga P. and Garcia-Roman, Miguel and Kunieda, Hironobu},
doi = {10.1016/j.colsurfa.2005.12.048},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Colloids and Surfaces A Physicochemical and Engineering Aspects/Engelskirchen et al. - 2006 - Effect of C12EOnmixed surfactant systems on the formation of viscoelastic worm-like micellar solutions.pdf:pdf},
issn = {09277757},
journal = {Colloids and Surfaces A: Physicochemical and Engineering Aspects},
keywords = {CTAB,Micellar growth,Nonionic surfactant,Sucrose surfactant,Viscoelastic micelles,Worm-like micelles},
number = {1-3},
pages = {113--120},
title = {{Effect of C12EOnmixed surfactant systems on the formation of viscoelastic worm-like micellar solutions in sucrose alkanoate- and CTAB-water systems}},
volume = {279},
year = {2006}
}
