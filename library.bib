 journal = {The Journal of Chemical Physics},
 number = {2},
 pages = {572-579},
 pmid = {25246403},
 title = {Reptation of a Polymer Chain in the Presence of Fixed Obstacles},
 volume = {55},
 year = {1971},
 source = {Crossref},
 url = {https://doi.org/10.1063/1.1675789},
 publisher = {AIP Publishing},
