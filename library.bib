doi = {10.1016/j.foodchem.2005.02.051},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Food Chemistry/Giangiacomo - 2006 - Study of water-sugar interactions at increasing sugar concentration by NIR spectroscopy.pdf:pdf},
