file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Carbohydrate Research/Broberg, Thomsen, Duus - 2000 - Application of nano-probe NMR for structure determination of low nanomole amounts of arabinoxylan oligos.pdf:pdf},
isbn = {0008-6215 (Print)$\backslash$r0008-6215 (Linking)},
issn = {00086215},
journal = {Carbohydrate Research},
keywords = {Arabinoxylan,Barley,HPAEC-PAD,MALDI-TOF MS,Nano-probe NMR,Oligosaccharide,Structure},
number = {3},
