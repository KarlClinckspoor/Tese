issn = {18734359},
journal = {Colloids and Surfaces A: Physicochemical and Engineering Aspects},
keywords = {Micelle,Mixed micelle,Organic dye,Solubilization,Surfactants (cationic, anionic, nonionic, and poly},
pages = {133--139},
pmid = {22444485},
