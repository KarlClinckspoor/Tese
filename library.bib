@article{Crist1973,
abstract = {The small-angle x-ray scattering (SAXS) patterns of a number of linear polyethylene (PE) and polyoxymethylene (POM) samples have been measured and compared to the intensity functions of one-dimensional paracrystalline lattices. It was found that the ratio of the angular positions of the second and first scattering maxima ($\theta$2/$\theta$1) is generally less than or equal to 2.0, implying that the paracrystalline lattice statistics are symmetric or moderately skewed to larger periods. The Bragg spacing (“long period”) of such samples is within 3{\%} of the identity period of the macrolattice. With quenched POM the ratio $\theta$2/$\theta$1 is substantially larger than 2.0, which indicates either extremely asymmetric lattice statistics or coexisting structures within the material. From consideration of the reduced widths of the first scattering maxima, it was found that some broadening is present in addition to that from the paracrystallinity. This excess broadening could result from a finite lattice length of ∼1000 {\AA}. The need for careful experimental technique for obtaining the actual position of the scattering maximum is emphasized. In addition, it is demonstrated that the scattering curve and the correlation function of the system yield essentially the same apparent structural periods.},
author = {Crist, B and Morosoff, N},
doi = {10.1002/pol.1973.180110601},
file = {:C$\backslash$:/Users/Karl/Google Drive/Doutorado/Artigos Mendeley/Journal of Polymer Science Polymer Physics Edition/Crist, Morosoff - 1973 - Small-angle x-ray scattering of semicrystalline polymers. II. Analysis of experimental scattering curves.pdf:pdf},
isbn = {1542-9385},
journal = {Journal of Polymer Science: Polymer Physics Edition},
number = {6},
pages = {1023--1045},
title = {{Small-angle x-ray scattering of semicrystalline polymers. II. Analysis of experimental scattering curves}},
url = {http://dx.doi.org/10.1002/pol.1973.180110601{\%}5Cnhttp://onlinelibrary.wiley.com/store/10.1002/pol.1973.180110601/asset/180110601{\_}ftp.pdf?v=1{\&}t=hesx9dh6{\&}s=cfa730fd0e781cadfde2f6e941008324b6e28a27},
volume = {11},
year = {1973}
}
