isbn = {1520-5827 (Electronic)$\backslash$n0743-7463 (Linking)},
issn = {07437463},
journal = {Langmuir},
number = {6},
pages = {3092--3101},
pmid = {22185602},
title = {{A stimulus-responsive shape-persistent micelle bearing a calix[4]arene building block: Reversible pH-dependent transition between spherical and cylindrical forms}},
